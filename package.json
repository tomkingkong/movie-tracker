  "private": true,
  "dependencies": {
    "connected-react-router": "^4.4.1",
    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "^1.3.0",
    "eslint": "^5.4.0",
    "history": "^4.7.2",
    "node-sass-chokidar": "^1.3.3",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
